{
  "extends": ["eslint:recommended", "prettier"],
  "plugins": ["prettier"],
  "env": {
    "node": true,
    "es2021": true,
    "jest": true
  },
  "rules": {
    "no-unused-vars": "warn",
    "prettier/prettier": "error",
    "no-undef": "error",
    "no-use-before-define": "error",
    "no-unreachable": "error",
    "no-unused-expressions": "error"
  }
}
